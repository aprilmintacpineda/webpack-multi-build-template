{
  "name": "multiple-builds-with-webpack",
  "version": "1.0.0",
  "description": "This project showcases how to use webpack to have more than one build for multiple environments.",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow"
    ],
    "plugins": [
      "babel-plugin-inferno",
      "@babel/plugin-syntax-jsx"
    ]
  },
  "scripts": {
    "reinstallNodeModules": "rm -rf node_modules package-lock.json && npm i",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf builds && babel-node node_modules/webpack-cli/bin/cli.js --config webpack.prod.js",
    "start": "babel-node node_modules/.bin/webpack-dev-server",
    "eslint": "eslint --fix src/**/*.js",
    "prettier": "prettier \"src/**/*.js\" --write",
    "lint": "npm run prettier && npm run eslint"
  },
  "author": "April Mintac Pineda",
  "license": "MIT",
  "dependencies": {
    "inferno": "^6.0.3",
    "inferno-router": "^6.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-inferno": "^6.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-inferno": "^7.11.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.14.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
